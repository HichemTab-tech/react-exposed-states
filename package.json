{
  "name": "react-exposed-states",
  "version": "1.0.1",
  "type": "module",
  "description": "A lightweight utility that lets you expose React state values to the browser window. Perfect for debugging and quick runtime tweaks during development.",
  "keywords": [
    "react",
    "react-states",
    "expose-states",
    "state-management",
    "react-package",
    "react-library"
  ],
  "main": "dist/main.js",
  "module": "dist/main.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.esm.js",
      "require": "./dist/main.min.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "preview": "vite preview",
    "dev:demo": "vite --config demo/vite.config.demo.ts",
    "build:demo": "vite build --config demo/vite.config.demo.ts"
  },
  "author": "Hichem Taboukouyout <hichem.taboukouyout@hichemtab-tech.me>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HichemTab-tech/react-exposed-states.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HichemTab-tech/react-exposed-states/issues"
  },
  "homepage": "https://github.com/HichemTab-tech/react-exposed-states#readme",
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "jsdom": "^27.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-plugin-banner": "^0.8.1",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "classnames": "^2.5.1"
  }
}
